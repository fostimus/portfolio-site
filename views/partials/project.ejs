<section class="<%= layout %> background-project">
  <div class="desc">
    <h2><%= title %></h2>
    <h4><%= date %></h4>
    <% btns.forEach(function(btn) { %>
      <a class="btn" href="<%= btn.link %>" target="_blank"><%= btn.text %></a>
    <% }) %>
  </div>
  <div class="project-image">
    <img src="<%= image %>" />
  </div>
</section>
